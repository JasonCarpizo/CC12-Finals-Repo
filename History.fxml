<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<VBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.HistoryController">
   <padding>
       <Insets top="10" right="10" bottom="10" left="10"/>
   </padding>
   <TableView fx:id="tableView">
       <columns>
           <TableColumn fx:id="dateColumn" text="Date"/>
           <TableColumn fx:id="productColumn" text="Product"/>
           <TableColumn fx:id="salePriceColumn" text="Sale Price"/>
       </columns>
   </TableView>
   <Button fx:id="backButton" text="Back" onAction="#handleBackButtonAction"/>
</VBox>
